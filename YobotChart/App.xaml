<Application
    x:Class="YobotChart.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Startup="Application_Startup"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/Colors/Theme/default.xaml"/>
                <ResourceDictionary Source="Styles/BrushDictionary.xaml"/>
                <ResourceDictionary Source="Styles/SvgDictionary.xaml" />
                <ResourceDictionary Source="UiComponents/ButtonComponent/CommonButton.xaml" />
                <ResourceDictionary Source="UiComponents/LoaderComponent/Loader.xaml" />
                <ResourceDictionary Source="UiComponents/TextBoxComponent/CommonTextBox.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <QuinticEase x:Key="QuinticEaseOut" EasingMode="EaseOut" />
            <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
